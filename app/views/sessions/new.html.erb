<div class="container">
  <h1>Sign in to view your account</h1>
  <div class="signup">
    <div class="sign-up-form">
      <class='navbar-form navbar-left'>
        <%= form_for(:sessions, url: sessions_path) do |f| %>
          <%= f.text_field :email, placeholder: "Email", class: 'form-control' %> <br>
          <%= f.password_field :password, placeholder: "Password", class: 'form-control' %> <br>
          <%= f.submit 'Sign in', class: 'btn btn-default navbar-btn' %>
        <% end %>
    </div>
  </div>
</div>
